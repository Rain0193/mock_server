{
	"apps" : [
    {
      "name"            : "mock_server",
      "script"          : "./bin/mock/run",
      "log_date_format" : "YYYY-MM-DD HH:mm Z",
      "out_file"        : "./logs/pm2-out.log",
      "error_file"      : "./logs/pm2-err.log",
      "pid_file"        : "./logs/ylws-admin.pid",
      "ignoreWatch"     : ["[\\/\\\\]\\./", "node_modules"],
      "watch"           : ["watch"],
      "exec_mode"       : "fork_mode", //cluster_mode
      "env": {
        "NODE_ENV"      : "production"
      }
    }
  ]
}
